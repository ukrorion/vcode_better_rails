{
  "injectionSelector": "L:text.haml",
  "patterns": [
    { "include": "#keywords" }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "support.function.rails render.actionpack.rails",
          "match": "(?<=\\=\\s)render(?=\\(|\\s)"
        },
        {
          "name": "support.function.rails helper.method.rails",
          "match": "(?<=\\=\\s)([a-z|_]+[?|!]?)(?=\\(|\\s)"
        }
      ]
    }
  },
  "scopeName": "embedded.rails"
}
