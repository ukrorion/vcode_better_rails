{
  "injectionSelector": "L:text.haml",
  "patterns": [
    { "include": "#entities" },
    { "include": "#keywords" }
  ],
  "repository": {
    "entities": {
      "patterns": [
        {
          "name": "support.function.activerecord.rails",
          "match": "\\b(link_to)\\b"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "support.function.actionpack.rails",
          "match": "(?<=\\=\\s)render(?=\\(|\\s)"
        },
        {
          "name": "support.function.actionpack.rails",
          "match": "(?<=\\=\\s)([a-z|_]+[?|!]?)(?=\\(|\\s)"
        }
      ]
    }
  },
  "scopeName": "haml.injection"
}
