{
	"injectionSelector": "L:source.ruby",
	"patterns": [
		{ "include": "#activerecord" }
	],
	"repository": {
		"activerecord": {
			"patterns": [
				{ "include": "#keywords" }
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "support.function.activerecord.rails",
					"match": "\\b(scope|has_many|belongs_to|has_secure_password|has_one)\\b"
				}
			]
		}
	},
	"scopeName": "rails.injection"
}
